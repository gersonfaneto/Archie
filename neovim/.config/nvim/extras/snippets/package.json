{
  "name": "custom-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "all",
        "path": "comments.json"
      },
      {
        "language": "sh",
        "path": "sh.json"
      },
      {
        "language": "license",
        "path": "license.json"
      },
      {
        "language": "markdown",
        "path": "markdown.json"
      },
      {
        "language": "gitcommit",
        "path": "gitcommit.json"
      }
    ]
  }
}
