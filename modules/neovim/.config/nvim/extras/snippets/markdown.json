{
  "Code Block": {
    "prefix": "code",
    "body": [
      "```$1",
      "$0",
      "```"
    ],
    "description": "Generate a code block."
  },
  "Marp": {
    "prefix": "marp",
    "body": [
      "---",
      "marp: true",
      "theme: ${1:uncover}",
      "class: ${2:invert}",
      "---",
      "$0"
    ],
    "description": "Generate a code block."
  },
  "Prettier Ignore - Block": {
    "prefix": "pib",
    "body": [
      "<!-- prettier-ignore -->"
    ],
    "description": "Make `prettier` ignore the next block."
  },
  "Prettier Ignore - Range": {
    "prefix": "pir",
    "body": [
      "<!-- prettier-ignore-start -->",
      "$0",
      "<!-- prettier-ignore-end -->"
    ],
    "description": "Make `prettier` ignore inside a range."
  }
}
